    "description": "Laravel Trust Component",
    "keywords": [
        "trust",
        "laravel"
    ],
    "license": "Apache License 2.0",
    "type": "library",
    "homepage": "https://github.com/Serhii-Podvysotskyi/laravel-trust",
    "authors": [
        {
            "name": "Serhii Podvysotskyi",
